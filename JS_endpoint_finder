javascript:(function(){     var r = /(?<=[%27"`])\/[a-zA-Z0-9._\-\/]+(?=[%27"`])/g;     var s = document.getElementsByTagName(%27script%27);     var urls = Array.from(s).filter(script => script.src).map(script => script.src);          var overlay = document.createElement(%27div%27);     overlay.style.cssText = %27position:fixed;top:20px;right:20px;width:350px;height:75vh;background:#1e1e1e;color:#00ff00;z-index:999999;overflow-y:auto;padding:15px;font-family:monospace;border:1px solid #444;box-shadow:0 10px 30px rgba(0,0,0,0.8);border-radius:5px;';     overlay.innerHTML = '<div style="display:flex;justify-content:space-between;border-bottom:1px solid #444;padding-bottom:10px;margin-bottom:10px;"><b>API Path Recon</b><button onclick="this.parentElement.parentElement.remove()" style="background:red;color:white;border:none;cursor:pointer;padding:2px 8px;border-radius:3px;">X</button></div><ul id="path-list" style="list-style:none;padding:0;margin:0;word-wrap:break-word;"><li>Scanning '+urls.length+' scripts...</li></ul>';     document.body.appendChild(overlay);          var list = document.getElementById('path-list');     var allPaths = new Set();          Promise.all(urls.map(url => fetch(url).then(res => res.text()).catch(() => '')))     .then(texts => {         texts.forEach(text => {             var matches = text.match(r);             if (matches) {                 matches.forEach(m => {                     if(m.length > 2 && !m.startsWith('//')) { allPaths.add(m); }                 });             }         });         list.innerHTML = '';         if(allPaths.size === 0) list.innerHTML = '<li>No clear paths found.</li>';         Array.from(allPaths).sort().forEach(path => {             var li = document.createElement('li');             li.textContent = path;             li.style.padding = '4px 0';             li.style.borderBottom = '1px dashed #333';             list.appendChild(li);         });     }); })();
