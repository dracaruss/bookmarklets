javascript:(function(){    /* Regex: Dictionary word -> optional suffix (_id) -> optional number (_123 or 123) */    var r = /["']?(?:id|uid|uuid|guid|gid|key|no|num|number|email|doc|file|record|group|product|user|account|order|customer|client|profile|transaction|edit)(?:[_-]?(?:id|no|num|key|uuid|uid))?(?:[_-]?\d+)?["%27]?\s*[:=]\s*["%27]?([a-zA-Z0-9_\-\.]{1,64})["%27]?/gi;    var found = new Set();    /* 1. Deep HTML & Inline Script Scan */    var html = document.documentElement.innerHTML;    var match;    while ((match = r.exec(html)) !== null) {        if(match[0].indexOf(%27{%27) === -1 && match[0].indexOf(%27;%27) === -1 && match[0].indexOf(%27<%27) === -1) {            found.add(%27DOM/JSON ➔ %27 + match[0]);        }    }    /* 2. Browser Storage Scan (Local & Session) */    [localStorage, sessionStorage].forEach(storage => {        for (let i = 0; i < storage.length; i++) {            let key = storage.key(i);            let val = storage.getItem(key);            if (val && typeof val === %27string%27) {                if (r.test(key) || r.test(val)) {                    let m = val.match(r);                    if(m) m.forEach(x => found.add(%27Storage ➔ %27 + x));                    let k = key.match(r);                    if(k) k.forEach(x => found.add(%27Storage Key ➔ %27 + x));                }            }        }    });    /* 3. The Hacker UI Overlay */    var overlay = document.createElement(%27div%27);    overlay.style.cssText = %27position:fixed;top:20px;left:20px;width:450px;max-height:75vh;background:#1a1a1a;color:#00ffcc;z-index:999999;overflow-y:auto;padding:15px;font-family:monospace;border:1px solid #333;box-shadow:0 10px 30px rgba(0,0,0,0.9);border-radius:5px;';    overlay.innerHTML = '<div style="display:flex;justify-content:space-between;border-bottom:1px solid #444;padding-bottom:10px;margin-bottom:10px;"><b>Deep IDOR Hunter V4</b><button onclick="this.parentElement.parentElement.remove()" style="background:#cc0000;color:white;border:none;cursor:pointer;padding:2px 8px;border-radius:3px;">X</button></div><ul id="idor-list" style="list-style:none;padding:0;margin:0;word-wrap:break-word;"></ul>';    document.body.appendChild(overlay);    var list = document.getElementById('idor-list');    if(found.size === 0) {        list.innerHTML = '<li>No obvious IDs found in DOM or Storage.</li>';    } else {        Array.from(found).sort().forEach(item => {            var li = document.createElement('li');            li.textContent = item;            li.style.padding = '5px 0';            li.style.borderBottom = '1px dashed #333';            list.appendChild(li);        });    }})();
